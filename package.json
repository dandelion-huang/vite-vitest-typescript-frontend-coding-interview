{
  "name": "vite-vitest-typescript-frontend-coding-interview",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write --cache .",
    "test:all": "vitest",
    "test:staged": "vitest run",
    "test": "vitest --dir",
    "test-01": "vitest --dir ./src/01-clamp",
    "test-02": "vitest --dir ./src/02-inRange"
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write --cache"
    ],
    "*.ts": [
      "npm run test:staged"
    ]
  },
  "devDependencies": {
    "husky": "^8.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vitest": "^1.4.0"
  }
}
